@using ReviewMe.ViewModel
@model ReviewMe.ViewModel.RoleViewModelLong
<h3> RoleList </h3>
<div>
@using (@Html.BeginForm("GetRoleList", "Role"))
{
    <table>
        <thead>
            <tr>
                <th>Role Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (RoleViewModel item in Model.RoleViewModelList)
            {
                <tr>
                    <td>item.RoleName</td>
                </tr>
            }
        </tbody>
    </table>
}
</div>